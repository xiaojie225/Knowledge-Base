.add2myApp .close-icon {
    position: relative;
    width: 28rpx;
    height: 28rpx;
    opacity: 0;
    animation: closeSwing 2s ease-out 3s forwards;
    animation-timing-function: cubic-bezier(0,.84,.58,.59);
}

.add2myApp .close-btn {
    position: absolute;
    left: -10rpx;
    top: 50%;
    padding: 12rpx;
    transform: translateY(-50%);
